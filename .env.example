# EduBridge Environment Configuration

# Google Gemini AI API Key
# Get your API key from: https://ai.google.dev/
VITE_API_KEY=your_google_gemini_api_key_here

# Application Settings
VITE_APP_NAME=EduBridge
VITE_APP_VERSION=2.0.0
VITE_ENVIRONMENT=development

# API Endpoints
VITE_API_TIMEOUT=30000
VITE_MAX_RETRIES=3

# RAG Configuration
VITE_RAG_ENABLED=true
VITE_RAG_VECTOR_DB=weaviate
VITE_KNOWLEDGE_BASE_SIZE=100

# ML Models Configuration
VITE_FACE_DETECTION_ENABLED=true
VITE_RESUME_PARSING_ENABLED=true
VITE_PEER_MATCHING_ENABLED=true

# AI Agents Configuration
VITE_COURSE_RECOMMENDATION_AGENT=true
VITE_JOB_MATCHING_AGENT=true
VITE_PEER_CONNECTION_AGENT=true
VITE_INTERVIEW_PREP_AGENT=true

# Feature Flags
VITE_ENABLE_DARK_MODE=true
VITE_ENABLE_BLIND_MODE=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_ANALYTICS=false

# Cache Settings
VITE_CACHE_RESPONSES=true
VITE_CACHE_TTL=3600000

# Debug Mode
VITE_DEBUG_MODE=false
VITE_LOG_LEVEL=info
